{
  "name": "bitress/laravel-semaphore",
  "description": "Laravel wrapper for Semaphore SMS API",
  "type": "library",
  "version": "1.0.2",
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^10.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bitress\\LaravelSemaphore\\": "src/"
    }
  },
  "authors": [
    {
      "name": "bitress",
      "email": "cyannejustinl.vega12@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Bitress\\LaravelSemaphore\\Providers\\SemaphoreServiceProvider"
      ],
      "aliases": {
        "SemaphoreSMS": "Bitress\\LaravelSemaphore\\Facades\\SemaphoreSMS"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
